/*
 * -------------------------------------------------
 *  Nextflow config file for running tests
 * -------------------------------------------------
 * Defines bundled input files and everything required
 * to run a fast and simple test. Use as follows:
 *   nextflow run nf-core/deepvariant -profile test
 */

params {
  max_cpus = 2
  max_memory = 6.GB
  max_time = 48.h
  bam = 'https://github.com/nf-core/test-datasets/raw/deepvariant/testdata/test.bam'
  genome = 'hg19chr20'
}
